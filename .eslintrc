{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "eqeqeq": 1,
    "no-plusplus": 0,
    "new-cap": 0,
    "camelcase": 0,
    "no-unused-vars": 0,
    "no-shadow": 0,
    "global-require": 0,
    "no-undef": 0,
    "no-console": 1,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2,
      }
    ]
  }
}

